<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>AzurePortalStyleNg</id>
    <version>0.0.11-beta</version>
    <authors>Harry Pfleger - Infoware</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Framework for building web sites based on the Azure Portal Layout.</description>
    <releaseNotes>Early preview.</releaseNotes>
    <dependencies>
      <dependency id="angularjs" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="AppAzurePortal\app.js" target="content\AppAzurePortal\app.js" />
    <file src="AppAzurePortal\config.js" target="content\AppAzurePortal\config.js" />

    <file src="AppAzurePortal\!!schema\navgrid.json" target="content\AppAzurePortal\!!schema\navgrid.json" />

    <file src="AppAzurePortal\portal\!directives\blade\blade.html" target="content\AppAzurePortal\portal\!directives\blade\blade.html" />
    <file src="AppAzurePortal\portal\!directives\blade\blade.js" target="content\AppAzurePortal\portal\!directives\blade\blade.js" />
    <file src="AppAzurePortal\portal\!directives\home\home.html" target="content\AppAzurePortal\portal\!directives\home\home.html" />
    <file src="AppAzurePortal\portal\!directives\home\home.js" target="content\AppAzurePortal\portal\!directives\home\home.js" />
    <file src="AppAzurePortal\portal\!directives\navgrid\navgrid.html" target="content\AppAzurePortal\portal\!directives\navgrid\navgrid.html" />
    <file src="AppAzurePortal\portal\!directives\navgrid\navgrid.js" target="content\AppAzurePortal\portal\!directives\navgrid\navgrid.js" />

    <file src="AppAzurePortalSample\sample.html" target="content\AppAzurePortalSample\sample.html" />
    <file src="AppAzurePortalSample\sample.js" target="content\AppAzurePortalSample\sample.js" />

    <file src="Content\images\avatar.jpg" target="content\Content\images\avatar.jpg" />
    <file src="Content\azureportalstyleng.css" target="content\Content\azureportalstyleng.css" />
  </files>
</package>